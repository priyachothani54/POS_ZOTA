ALTER   proc [dbo].[SP_VendorDetial]
@status NVARCHAR(MAX) = ''
As
Begin
	Begin
		if @status = 'VendorDetail'
		begin
			select v.Vendor_Code,v.Vendor_Name,v.Vendor_Address,v.Vendor_Type from Vendor_Detail v (NOLOCK);
		end
		
	end
End
